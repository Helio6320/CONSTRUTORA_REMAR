@model List<Cliente>
<h1>Clientes cadastrados no sistema</h1>

<p class="text-danger">@ViewData["Mensagem"]</p>

<table class="table">
    <thead>
        <tr>
            <th scope="row">Nome</th>
            <th scope="row">Login</th>
            <th scope="row">Tipo</th>
        </tr>
    </thead>

    <tbody>
        @foreach (Cliente c in Model)
        {
        <tr>
            <td>@c.Nome</td>
            <td>@c.Login</td>
            @if(c.Tipo==Cliente.ADMIN)
            {
                <td>Administrator</td>
            }
            else
            {
                <td>Padr√£o<object data="" type=""></object></td>
            }

            <td><a href="editarCliente?id=@c.Id">editar</a></td>
            <td><a href="ExcluirCliente?id=@c.Id">excluir</a></td>
        </tr>
        }
    </tbody>
</table>

<a href="RegistrarClientes">Registrar novo Cliente</a>